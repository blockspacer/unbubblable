{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "THREE": true,
        "Stats": true
    },
    "rules": {
        "no-mixed-operators": "off",
        "default-case": "off",
        "no-plusplus": "off"
    }
}